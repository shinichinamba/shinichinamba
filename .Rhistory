get_bib <- function(title, doi, pmid) {
tmp <- tempfile(fileext = ".bib")
if (!missing(title)) {
x <- RefManageR::ReadPubMed(title)
WriteBib(x, tmp)
} else if (!missing(doi)) {
GetBibEntryWithDOI(doi, temp.file = tmp, delete.file = FALSE)
